---
import { Routes } from "@data/navLinks";
import DitComercial from "@icons/DitComercial.astro";
import DitOutsourcing from "@icons/DitOutsourcing.astro";
import DitTransport from "@icons/DitTransport.astro";
import { Image } from "astro:assets";
---

<section
  class="flex flex-wrap justify-center gap-8 p-8 mx-auto my-0"
>
  <a class="w-72" href={Routes.Transporte}>
    <DitTransport />
  </a>
  <a class="w-72" href={Routes.Outsourcing}>
    <DitOutsourcing />
  </a>
  <a class="w-72" href={Routes.Comercial}>
    <DitComercial />
  </a>
</section>
